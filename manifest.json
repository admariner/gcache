{
"name" :"GCA",
"version" :"1.0",
"description" :"test de cache",
"manifest_version": 2,
"background": { "scripts": ["background.js"] },
"content_scripts": [
    {
		"matches": ["<all_urls>"],
		"js": ["cache.js"],
		"default_icon" : "off.png",
		"run_at" : "document_end"
    }
  ],
"browser_action" :
	{
		"default_icon" : "off.png",
		"name": "Click to toggle offline mode"
	}

}